<resources>

    <!-- Like in themes_base.xml, the namespace "Base.AppCompat.*" is used to
         define base styles for the platform version. The "*.AppCompat"
         variants are for direct use or use as parent styles by the app. -->
    <eat-comment/>

    <style name="Base.Widget.AppCompat.ActionBar" parent="">
        <item name="displayOptions">showTitle</item>
        <item name="height">?attr/actionBarSize</item>

        <item name="titleTextStyle">@style/TextAppearance.AppCompat.Widget.ActionBar.Title</item>
        <item name="subtitleTextStyle">@style/TextAppearance.AppCompat.Widget.ActionBar.Subtitle</item>

        <item name="background">@drawable/sesl_action_bar_background_dark</item>
        <item name="backgroundStacked">@drawable/sesl_action_bar_background_stacked_dark</item>

        <item name="actionButtonStyle">@style/Widget.AppCompat.ActionButton</item>
        <item name="actionOverflowButtonStyle">@style/Widget.AppCompat.ActionButton.Overflow</item>

        <item name="contentInsetStart">@dimen/sesl_action_bar_content_inset</item>
        <item name="contentInsetStartWithNavigation">@dimen/sesl_action_bar_content_inset_with_nav</item>
        <item name="contentInsetEnd">0dp</item>
        <item name="elevation">@dimen/sesl_action_bar_elevation</item>
    </style>

    <style name="Base.Widget.AppCompat.Light.ActionBar" parent="Base.Widget.AppCompat.ActionBar">
        <item name="actionButtonStyle">@style/Widget.AppCompat.Light.ActionButton</item>
        <item name="actionOverflowButtonStyle">@style/Widget.AppCompat.Light.ActionButton.Overflow</item>
        <item name="background">@drawable/sesl_action_bar_background_light</item>
        <item name="backgroundStacked">@drawable/sesl_action_bar_background_stacked_light</item>
        <item name="subtitleTextStyle">@style/TextAppearance.AppCompat.Widget.ActionBar.Subtitle.Inverse</item>
        <item name="titleTextStyle">@style/TextAppearance.AppCompat.Widget.ActionBar.Title.Inverse</item>
    </style>

    <style name="Base.Widget.AppCompat.ActionBar.Solid" parent="Base.Widget.AppCompat.ActionBar">
        <item name="background">?attr/colorPrimary</item>
        <item name="backgroundStacked">?attr/colorPrimary</item>
        <item name="backgroundSplit">?attr/colorPrimary</item>
    </style>

    <style name="Base.Widget.AppCompat.Light.ActionBar.Solid" parent="Base.Widget.AppCompat.Light.ActionBar">
        <item name="background">?attr/colorPrimary</item>
        <item name="backgroundStacked">?attr/colorPrimary</item>
        <item name="backgroundSplit">?attr/colorPrimary</item>
    </style>

    <style name="Base.Widget.AppCompat.ActionButton" parent="android:Widget.Material.ActionButton">
        <item name="android:background">?attr/actionBarItemBackground</item>
        <item name="android:minWidth">@dimen/sesl_action_button_min_width</item>
        <item name="android:minHeight">?attr/actionBarSize</item>
        <item name="paddingStart">@dimen/sesl_action_button_padding_horizontal</item>
        <item name="paddingEnd">@dimen/sesl_action_button_padding_horizontal</item>
        <item name="android:scaleType">center</item>
        <item name="android:gravity">center</item>
        <item name="android:maxLines">2</item>
        <item name="textAllCaps">false</item>
    </style>

    <style name="Base.Widget.AppCompat.ActionButton.CloseMode" parent="Base.Widget.AppCompat.ActionButton">
        <item name="android:background">?attr/actionBarItemBackground</item>
        <item name="android:minWidth">@dimen/sesl_action_button_min_width</item>
        <item name="android:minHeight">?actionBarSize</item>
    </style>

    <style name="Base.Widget.AppCompat.ActionButton.Overflow" parent="Base.Widget.AppCompat.ActionButton">
        <item name="android:src">@drawable/sesl_ic_menu_overflow_dark</item>
        <item name="android:background">?attr/actionBarItemBackground</item>
        <item name="android:contentDescription">@string/sesl_action_menu_overflow_description</item>
        <item name="android:minWidth">@dimen/sesl_overflow_button_min_width</item>
        <item name="android:minHeight">?attr/actionBarSize</item>
