    <!-- Spinner Widgets -->

    <style name="Platform.Widget.AppCompat.Spinner" parent="android:Widget.Spinner.DropDown" />

    <style name="Base.Widget.AppCompat.Spinner" parent="Platform.Widget.AppCompat.Spinner">
        <item name="android:background">@drawable/sesl_spinner_background_dark</item>
        <item name="android:popupBackground">@drawable/sesl_menu_popup_background_dark</item>
        <item name="android:popupAnimationStyle">@style/Base.Animation.AppCompat.ListPopup</item>
